{
  "name": "enebular-agent-runner",
  "version": "2.8.0",
  "description": "The runner to launch enebular-agent",
  "repository": "enebular/enebular-runtime-agent",
  "main": "lib/index.js",
  "scripts": {
    "prepare": "npm run build",
    "start": "node lib/index.js",
    "build": "npm run build:clean && tsc -p tsconfig.build.json",
    "build:clean": "rimraf lib",
    "lint": "eslint ./src --ext .js,.ts",
    "lint-fix": "eslint ./src --ext .js,.ts --fix",
    "lint-test": "eslint ./test --ext .js,.ts --fix",
    "typecheck": "tsc --noEmit",
    "test": "npm run build:clean && tsc && ava",
    "test-report": "npm run test -- --tap | npx tap-spec"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "9.2.1"
  },
  "devDependencies": {
    "@types/node": "^12.7.4",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "ava": "^2.3.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "tap-spec": "^5.0.0",
    "typescript": "^3.6.2"
  },
  "dependencies": {}
}
