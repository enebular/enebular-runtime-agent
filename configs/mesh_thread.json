{
    "macros": [
        "MBEDTLS_USER_CONFIG_FILE=\"mbedtls_mbed_client_config.h\"",
        "MBED_CLIENT_USER_CONFIG_FILE=\"mbed_cloud_client_user_config.h\"",
        "MBED_CLOUD_CLIENT_USER_CONFIG_FILE=\"mbed_cloud_client_user_config.h\"",
        "PAL_DTLS_PEER_MIN_TIMEOUT=10000",
        "MBED_CONF_APP_MAIN_STACK_SIZE=4608"
    ],
    "target_overrides": {
        "*": {
            "target.features_add": ["NANOSTACK"],
            "nanostack.configuration": "thread_router",
            "mbed-mesh-api.thread-config-channel": 22,
            "mbed-mesh-api.thread-config-panid": "0x0700",
            "platform.stdio-baud-rate": 115200,
            "platform.stdio-convert-newlines": true,
            "mbed-client.event-loop-size": 32768,
            "mbed-client.reconnection-count": 6,
            "mbed-client.dns-use-thread": 1,
            "mbed-client.sn-coap-blockwise-max-time-data-stored": 120,
            "mbed-trace.enable": null,
            "storage-selector.filesystem": "FAT",
            "storage-selector.mount-point": "\"sd\"",
            "storage-selector.storage": "SD_CARD"
        },
        "K64F": {
            "target.mbed_app_start": "0x00020400",
            "update-client.application-details": "0x00020000",
            "update-client.bootloader-details": "0x172e4"
        },
        "NUCLEO_F429ZI": {
            "target.mbed_app_start": "0x08020400",
            "update-client.application-details": "0x08020000",
            "update-client.bootloader-details": "0x8018630",
            "target.macros_add": ["ATMEL_SPI_MOSI=PB_5"]
        },
        "UBLOX_EVK_ODIN_W2": {
            "target.mbed_app_start": "0x08020400",
            "update-client.application-details": "0x08020000",
            "update-client.bootloader-details": "0x801ae78"
        }
    },
    "config": {
        "network-interface":{
            "value": "MESH_THREAD"
        },
        "mesh_radio_type": {
            "help": "options are ATMEL, MCR20, SPIRIT1",
            "value": "ATMEL"
        },
        "developer-mode": {
            "help": "Enable Developer mode to skip Factory enrollment",
            "value": 1
        },
        "button-pinname": {
            "help": "PinName for button.",
            "value": "BUTTON1"
        },
        "led-pinname": {
            "help": "PinName for led, which is attached to led blink resource.",
            "value": "LED_RED"
        }
    }
}

