{
  "name": "enebular-runtime-agent",
  "version": "2.14.0",
  "description": "The enebular-agent core for enebular",
  "main": "lib/index",
  "scripts": {
    "build": "npm run build:clean && npm run build:lib && npm run build:flow && npm run build:ts",
    "build:clean": "rimraf lib",
    "build:lib": "babel -d lib src",
    "build:flow": "flow-copy-source -v src lib",
    "build:ts": "tsc -p tsconfig.build.json",
    "flow": "flow",
    "lint": "eslint src --ext .js",
    "lint:ts": "eslint src --ext .ts -c .eslintrc-ts.json",
    "lint-fix": "eslint src --ext .js --fix",
    "lint:ts-fix": "eslint src --ext .ts -c .eslintrc-ts.json --fix",
    "test": "npm run test:ava && npm run test:jest",
    "test:ava": "ava --serial",
    "test:jest": "jest --runInBand --forceExit"
  },
  "engines": {
    "node": "12.14.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@types/jest": "^24.0.12",
    "@types/node": "^12.7.4",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.2.0",
    "ava": "^1.2.1",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^25.1.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-flow": "^2.29.1",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "express": "^4.15.3",
    "flow-bin": "^0.102.0",
    "flow-copy-source": "^1.3.0",
    "get-folder-size": "^2.0.0",
    "jest": "^24.9.0",
    "multer": "^1.3.0",
    "prettier": "^1.18.2",
    "sinon": "^7.5.0",
    "tap-spec": "^5.0.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.6.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "commander": "^2.16.0",
    "diskusage": "^1.1.3",
    "dockerode": "^2.5.8",
    "extract-zip": "^1.6.7",
    "form-data": "^2.3.2",
    "get-port": "^5.0.0",
    "ip": "^1.1.5",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^8.2.1",
    "mkdirp": "^0.5.1",
    "object-hash": "^1.3.0",
    "object-path": "^0.11.4",
    "portfinder": "^1.0.20",
    "request": "^2.88.0",
    "request-progress": "^3.0.0",
    "rimraf": "^3.0.0",
    "string-argv": "^0.1.1",
    "unix-dgram": "^2.0.3",
    "winston": "^3.2.1",
    "winston-compat": "^0.1.4",
    "winston-syslog": "^2.3.0",
    "fs-extra": "^9.0.0"
  },
  "ava": {
    "files": [
      "test/*.test.js"
    ],
    "require": [
      "@babel/register"
    ]
  }
}
