{
  "name": "enebular-agent-updater",
  "version": "2.18.2",
  "description": "The client to update enebular-agent",
  "repository": "enebular/enebular-agent-updater",
  "main": "lib/src/index.js",
  "scripts": {
    "start": "node lib/src/index.js",
    "build": "npm run build:clean && tsc -p tsconfig.build.json",
    "build:clean": "rimraf lib",
    "lint": "eslint ./src --ext .js,.ts",
    "lint-fix": "eslint ./src --ext .js,.ts --fix",
    "lint-test": "eslint ./test --ext .js,.ts --fix",
    "typecheck": "tsc --noEmit",
    "test": "npm run build:clean && tsc && ava",
    "test-report": "npm run test -- --tap | npx tap-spec"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "22.17.1"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "ava": "^5.3.0",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "prettier": "^1.16.4",
    "tap-spec": "^5.0.0",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "check-disk-space": "^1.5.0",
    "commander": "^10.0.0",
    "isomorphic-fetch": "^3.0.0",
    "rimraf": "^2.6.3"
  },
  "overrides": {
    "braces": "^3.0.3",
    "cross-spawn": "^7.0.6",
    "got": "^11.8.6",
    "tough-cookie": "^4.1.4",
    "trim": "1.0.1",
    "trim-newlines": "^5.0.0",
    "yargs-parser": "^13.1.2"
  },
  "ava": {
    "files": [
      "lib/test/*.test.js"
    ]
  }
}